<aura:component description="campingHeader" controller="LocalStorageController" implements="flexipage:availableForAllPageTypes" access="global">

    <ltng:require scripts="{!$Resource.secure_js + '/secure_js.js'}" afterScriptsLoaded="{!c.afterScriptsLoaded}"/>
    <aura:attribute name="objSecure" type="Object"/>
    <H1>Camping List </H1>
    <button onclick="{!c.fetchLocalStorageValues}">Get Name</button>
    <iframe src="/apex/AccountsTab" width="100%" height="100%"/>
</aura:component>